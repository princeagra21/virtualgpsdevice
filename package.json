{
  "name": "gps-virtual-device-sender",
  "version": "1.0.0",
  "description": "Production-ready GPS device simulator with protocol support",
  "main": "dist/app/electron/main.js",
  "scripts": {
    "dev": "tsc -p tsconfig.electron.json && concurrently \"next dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "build": "prisma generate && next build && tsc -p tsconfig.electron.json",
    "build:exe": "npm run build && electron-builder --win --dir"
  },
  "keywords": [
    "gps",
    "tracker",
    "simulator",
    "gt06",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^11.10.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.gps.virtual.device.sender",
    "productName": "GPS Virtual Device Sender",
    "forceCodeSigning": false,
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      ".next/**/*",
      "prisma/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/**/*",
      "assets/**/*",
      "public/**/*"
    ],
    "asarUnpack": [
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*",
      "prisma/**/*",
      ".next/**/*",
      "assets/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico",
      "sign": null
    },
    "mac": {
      "target": [
        "dmg",
        "pkg"
      ],
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development"
    }
  }
}
